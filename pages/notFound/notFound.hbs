{{#extend "base"}}
    {{#content "title"}}
        <title>Не найдено</title>
    {{/content}}

    {{#content "styles"}}
        {{#if commonData.isDev}}
            <link rel="stylesheet" href="/notFound.css">
        {{else}}
            <link rel="stylesheet" href="//dream-team-4.surge.sh/notFound.css">
        {{/if}}
    {{/content}}

    {{#content "main"}}
        <div class="box main-div">
            <img src="http://iconspot.ru/image.php?width=512&height=512&crop=none&id=7196">
            <div class="message">
                {{{text}}}
            </div>
        </div>
    {{/content}}

    {{#content "scripts"}}
        {{#if commonData.isDev}}
            <script src="/notFound.js"></script>
        {{else}}
            <script src="//dream-team-4.surge.sh/notFound.js"></script>
        {{/if}}
    {{/content}}
{{/extend }}
